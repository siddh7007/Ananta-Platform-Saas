# Webhook Bridge Service Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
PORT=27600

# Webhook Security
# This secret is used to verify webhook signatures from Control Plane
# Must match the value configured in tenant-management-service
WEBHOOK_SECRET=your-webhook-secret-change-me

# Supabase Configuration
# URL for the Supabase instance (PostgREST endpoint)
SUPABASE_URL=http://localhost:27540

# Supabase service_role key (REQUIRED)
# This key bypasses RLS and is needed for provisioning operations
# Get this from your Supabase project settings or docker-compose output
SUPABASE_SERVICE_KEY=

# Temporal Configuration
# Address of the Temporal server
TEMPORAL_ADDRESS=localhost:7233
