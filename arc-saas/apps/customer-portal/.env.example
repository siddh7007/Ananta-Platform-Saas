# CBP Customer Portal Environment Variables

# Keycloak Configuration
VITE_KEYCLOAK_URL=http://localhost:8180
VITE_KEYCLOAK_REALM=ananta
VITE_KEYCLOAK_CLIENT_ID=cbp-frontend

# API URLs
VITE_API_URL=http://localhost:14000
# CNS API base URL (hook appends /api/enrichment/stream)
VITE_CNS_API_URL=http://localhost:27200
VITE_SUPABASE_URL=http://localhost:27810

# Feature Flags
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_MOCK_DATA=false

# App Configuration
VITE_APP_NAME=Component Platform
VITE_APP_VERSION=0.1.0

# API Timeout Configuration (milliseconds)
VITE_API_TIMEOUT=30000
VITE_API_SEARCH_TIMEOUT=15000

# OpenTelemetry Configuration
# Uses shared arc-saas Jaeger (start with: docker-compose up -d jaeger)
# Set to 'false' to disable telemetry in development
VITE_OTEL_ENABLED=true
# OTLP endpoint - points to arc-saas-jaeger container (port 4318)
VITE_OTEL_EXPORTER_ENDPOINT=http://localhost:4318/v1/traces
# Service name for traces (appears in Jaeger UI at http://localhost:16686)
VITE_SERVICE_NAME=cbp-frontend

# Error Tracking (Sentry)
# VITE_SENTRY_DSN=https://xxx@sentry.io/xxx

# Novu Notification Configuration
VITE_NOVU_APP_IDENTIFIER=6931905380e6f7e26e0ddaad
VITE_NOVU_API_URL=http://localhost:13100
VITE_NOVU_WS_URL=http://localhost:13101
