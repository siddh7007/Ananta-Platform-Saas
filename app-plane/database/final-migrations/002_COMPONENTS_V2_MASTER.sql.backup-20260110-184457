-- ==========================================
-- NOTE: DIRECTUS TABLES ARE IN SEPARATE DATABASE
-- ==========================================
-- Directus uses its own database called "directus" (not components_v2)
-- Directus auto-creates all directus_* tables on startup
-- See docker-compose.yml: DB_DATABASE: directus
-- Do NOT add directus_* tables here

-- ==========================================
-- REDIS CONFIGURATION TABLES
-- ==========================================
-- These tables store Redis config/settings in PostgreSQL
-- The actual cache data lives in Redis (in-memory)

-- Redis Cache Config
CREATE TABLE IF NOT EXISTS redis_cache_config (
    id SERIAL PRIMARY KEY,
    service_name VARCHAR(100) NOT NULL,
    redis_namespace VARCHAR(255) NOT NULL,
    ttl_seconds INTEGER DEFAULT 3600,
    enabled BOOLEAN DEFAULT true,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis Circuit Breakers
CREATE TABLE IF NOT EXISTS redis_circuit_breakers (
    id SERIAL PRIMARY KEY,
    service_name VARCHAR(100) NOT NULL,
    state VARCHAR(20) DEFAULT 'closed' NOT NULL,
    failure_count INTEGER DEFAULT 0,
    failure_threshold INTEGER DEFAULT 5 NOT NULL,
    last_failure_time TIMESTAMPTZ,
    recovery_attempt_time TIMESTAMPTZ,
    opened_at TIMESTAMPTZ,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis CNS Config
CREATE TABLE IF NOT EXISTS redis_cns_config (
    id SERIAL PRIMARY KEY,
    service_name VARCHAR(50) DEFAULT 'cns-service' NOT NULL,
    cache_ttl_seconds INTEGER DEFAULT 3600 NOT NULL,
    lookup_cache_ttl_seconds INTEGER DEFAULT 86400 NOT NULL,
    batch_cache_ttl_seconds INTEGER DEFAULT 1800 NOT NULL,
    compression_enabled BOOLEAN DEFAULT true NOT NULL,
    max_cache_size_mb INTEGER DEFAULT 2048 NOT NULL,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis Component Cache Config
CREATE TABLE IF NOT EXISTS redis_component_cache_config (
    id SERIAL PRIMARY KEY,
    entity_type VARCHAR(50) NOT NULL,
    ttl_seconds INTEGER DEFAULT 86400 NOT NULL,
    update_on_write BOOLEAN DEFAULT true NOT NULL,
    update_on_read BOOLEAN DEFAULT false NOT NULL,
    compression_enabled BOOLEAN DEFAULT true NOT NULL,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis Component Snapshot
CREATE TABLE IF NOT EXISTS redis_component_snapshot (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    redis_key TEXT NOT NULL,
    line_id TEXT NOT NULL,
    mpn TEXT,
    manufacturer TEXT,
    quality_score NUMERIC(5,2),
    component_data JSONB NOT NULL,
    storage_ttl_seconds INTEGER,
    expires_at TIMESTAMPTZ,
    reason_for_redis TEXT,
    can_promote BOOLEAN DEFAULT true,
    promotion_notes TEXT,
    last_synced_at TIMESTAMPTZ DEFAULT NOW(),
    sync_status TEXT DEFAULT 'active',
    created_at TIMESTAMPTZ DEFAULT NOW()
);

COMMENT ON TABLE redis_component_snapshot IS 'Snapshot of components stored in Redis (temporary, low-quality) for Directus visibility';
COMMENT ON COLUMN redis_component_snapshot.can_promote IS 'Whether admin can manually promote this component to permanent storage';

-- Redis Connection Config
CREATE TABLE IF NOT EXISTS redis_connection_config (
    id SERIAL PRIMARY KEY,
    config_name VARCHAR(100) NOT NULL,
    host VARCHAR(255) DEFAULT 'redis',
    port INTEGER DEFAULT 6379,
    database INTEGER DEFAULT 0,
    password_encrypted VARCHAR(500),
    connection_pool_size INTEGER DEFAULT 10,
    max_overflow INTEGER DEFAULT 5,
    timeout_seconds INTEGER DEFAULT 5,
    retry_on_timeout BOOLEAN DEFAULT true,
    ssl_enabled BOOLEAN DEFAULT false,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis Directus Config
CREATE TABLE IF NOT EXISTS redis_directus_config (
    id SERIAL PRIMARY KEY,
    collection_name VARCHAR(100) NOT NULL,
    ttl_seconds INTEGER DEFAULT 3600 NOT NULL,
    invalidate_on_webhook BOOLEAN DEFAULT true NOT NULL,
    batch_operations BOOLEAN DEFAULT true NOT NULL,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis Health Check
CREATE TABLE IF NOT EXISTS redis_health_check (
    id SERIAL PRIMARY KEY,
    service_name VARCHAR(100),
    redis_db INTEGER,
    connection_status VARCHAR(50),
    response_time_ms INTEGER,
    memory_used_mb NUMERIC(10,2),
    keys_count INTEGER,
    last_check TIMESTAMPTZ,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis Health Checks (History)
CREATE TABLE IF NOT EXISTS redis_health_checks (
    id SERIAL PRIMARY KEY,
    check_timestamp TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    service_name VARCHAR(50) NOT NULL,
    status VARCHAR(20) NOT NULL,
    response_time_ms INTEGER,
    last_error TEXT,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis Key Expiration Policy
CREATE TABLE IF NOT EXISTS redis_key_expiration_policy (
    id SERIAL PRIMARY KEY,
    key_pattern VARCHAR(255) NOT NULL,
    ttl_seconds INTEGER NOT NULL,
    eviction_policy VARCHAR(50) DEFAULT 'allkeys-lru',
    description TEXT,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis Rate Limits
CREATE TABLE IF NOT EXISTS redis_rate_limits (
    id SERIAL PRIMARY KEY,
    supplier_name VARCHAR(100) NOT NULL,
    requests_per_hour INTEGER DEFAULT 10000 NOT NULL,
    requests_per_minute INTEGER DEFAULT 200 NOT NULL,
    burst_allowance INTEGER DEFAULT 10 NOT NULL,
    throttle_duration_ms INTEGER DEFAULT 1000 NOT NULL,
    enabled BOOLEAN DEFAULT true NOT NULL,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis S3 Metadata Config
CREATE TABLE IF NOT EXISTS redis_s3_metadata_config (
    id SERIAL PRIMARY KEY,
    bucket_name VARCHAR(100) NOT NULL,
    ttl_seconds INTEGER DEFAULT 3600 NOT NULL,
    cache_headers BOOLEAN DEFAULT true NOT NULL,
    cache_tags BOOLEAN DEFAULT true NOT NULL,
    max_object_size_mb INTEGER DEFAULT 100 NOT NULL,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis Sync Lock
CREATE TABLE IF NOT EXISTS redis_sync_lock (
    lock_name TEXT PRIMARY KEY,
    locked_by TEXT NOT NULL,
    locked_at TIMESTAMPTZ DEFAULT NOW(),
    expires_at TIMESTAMPTZ NOT NULL
);

COMMENT ON TABLE redis_sync_lock IS 'Advisory locks for Redis sync operations to prevent race conditions';

-- Redis Sync Log
CREATE TABLE IF NOT EXISTS redis_sync_log (
    id SERIAL PRIMARY KEY,
    service_name VARCHAR(100),
    operation VARCHAR(50),
    redis_key VARCHAR(500),
    data_size_bytes INTEGER,
    success BOOLEAN,
    error_message TEXT,
    sync_time INTEGER,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Redis Temporal Config
CREATE TABLE IF NOT EXISTS redis_temporal_config (
    id SERIAL PRIMARY KEY,
    service_name VARCHAR(50) NOT NULL,
    redis_host VARCHAR(100) DEFAULT 'localhost' NOT NULL,
    redis_port INTEGER DEFAULT 6379 NOT NULL,
    redis_db INTEGER DEFAULT 0 NOT NULL,
    connection_pool_size INTEGER DEFAULT 10 NOT NULL,
    timeout_ms INTEGER DEFAULT 5000 NOT NULL,
    retry_policy JSONB DEFAULT '{"backoff_ms": 100, "max_retries": 3, "backoff_multiplier": 2.0}' NOT NULL,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- ==========================================
-- ADDITIONAL CNS TABLES
-- ==========================================

-- S3 Cache Config
CREATE TABLE IF NOT EXISTS s3_cache_config (
    id SERIAL PRIMARY KEY,
    bucket_name VARCHAR(100) NOT NULL,
    cache_path VARCHAR(255),
    ttl_seconds INTEGER DEFAULT 86400 NOT NULL,
    max_file_size_mb INTEGER DEFAULT 100,
    compression_enabled BOOLEAN DEFAULT true,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- AI Prompts (for AI-enhanced enrichment)
CREATE TABLE IF NOT EXISTS ai_prompts (
    id SERIAL PRIMARY KEY,
    name VARCHAR(255) NOT NULL UNIQUE,
    description TEXT,
    system_prompt TEXT,
    messages JSON,
    model VARCHAR(100) DEFAULT 'gpt-4',
    temperature NUMERIC(3,2) DEFAULT 0.7,
    max_tokens INTEGER DEFAULT 2000,
    is_active BOOLEAN DEFAULT true,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Enrichment Batch Jobs
CREATE TABLE IF NOT EXISTS enrichment_batch_jobs (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    organization_id UUID,
    bom_id UUID,
    status VARCHAR(50) DEFAULT 'pending' NOT NULL,
    total_items INTEGER DEFAULT 0,
    processed_items INTEGER DEFAULT 0,
    successful_items INTEGER DEFAULT 0,
    failed_items INTEGER DEFAULT 0,
    start_time TIMESTAMPTZ,
    end_time TIMESTAMPTZ,
    error_message TEXT,
    temporal_workflow_id TEXT,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

CREATE INDEX IF NOT EXISTS idx_enrichment_batch_jobs_status ON enrichment_batch_jobs(status);
CREATE INDEX IF NOT EXISTS idx_enrichment_batch_jobs_org ON enrichment_batch_jobs(organization_id);

-- Enrichment Queue (Components-V2 side)
CREATE TABLE IF NOT EXISTS enrichment_queue (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    mpn TEXT NOT NULL,
    manufacturer TEXT,
    priority INTEGER DEFAULT 5 CHECK (priority >= 1 AND priority <= 10),
    status VARCHAR(50) DEFAULT 'pending' NOT NULL,
    source VARCHAR(50) DEFAULT 'manual',
    organization_id UUID,
    bom_id UUID,
    batch_job_id UUID REFERENCES enrichment_batch_jobs(id),
    attempts INTEGER DEFAULT 0,
    last_attempt_at TIMESTAMPTZ,
    error_message TEXT,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

CREATE INDEX IF NOT EXISTS idx_enrichment_queue_status ON enrichment_queue(status);
CREATE INDEX IF NOT EXISTS idx_enrichment_queue_priority ON enrichment_queue(priority DESC);

-- Enrichment History (Components-V2 side)
CREATE TABLE IF NOT EXISTS enrichment_history (
    id SERIAL PRIMARY KEY,
    mpn VARCHAR(255) NOT NULL,
    manufacturer VARCHAR(255),
    enrichment_data JSONB,
    quality_score NUMERIC(5,2),
    status VARCHAR(50) NOT NULL,
    rejection_reason TEXT,
    error_message TEXT,
    issues JSONB DEFAULT '[]',
    enrichment_source VARCHAR(50),
    bom_job_id VARCHAR(100),
    api_calls JSONB DEFAULT '[]',
    processing_time_ms INTEGER,
    tier_reached INTEGER,
    created_at TIMESTAMPTZ DEFAULT NOW(),
    created_by UUID
);

CREATE INDEX IF NOT EXISTS idx_enrichment_history_mpn ON enrichment_history(mpn);
CREATE INDEX IF NOT EXISTS idx_enrichment_history_status ON enrichment_history(status);

COMMENT ON TABLE enrichment_history IS 'Audit log of all enrichment attempts (approved, rejected, errors)';

-- Component Lifecycle
CREATE TABLE IF NOT EXISTS component_lifecycle (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    component_id UUID REFERENCES components(id) ON DELETE CASCADE,
    mpn TEXT NOT NULL,
    manufacturer TEXT,
    lifecycle_status VARCHAR(50) DEFAULT 'active',
    introduction_date DATE,
    end_of_life_date DATE,
    last_buy_date DATE,
    replacement_mpn TEXT,
    notes TEXT,
    data_source VARCHAR(100),
    confidence_score NUMERIC(5,2),
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

CREATE INDEX IF NOT EXISTS idx_component_lifecycle_mpn ON component_lifecycle(mpn);
CREATE INDEX IF NOT EXISTS idx_component_lifecycle_status ON component_lifecycle(lifecycle_status);

-- Supplier Performance Metrics
CREATE TABLE IF NOT EXISTS supplier_performance_metrics (
    id SERIAL PRIMARY KEY,
    supplier_id INTEGER REFERENCES suppliers(id),
    supplier_name VARCHAR(100) NOT NULL,
    date DATE NOT NULL,
    total_requests INTEGER DEFAULT 0,
    successful_requests INTEGER DEFAULT 0,
    failed_requests INTEGER DEFAULT 0,
    avg_response_time_ms INTEGER,
    avg_quality_score NUMERIC(5,2),
    uptime_percentage NUMERIC(5,2),
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    UNIQUE(supplier_name, date)
);

CREATE INDEX IF NOT EXISTS idx_supplier_performance_date ON supplier_performance_metrics(date);

-- Supplier Settings
CREATE TABLE IF NOT EXISTS supplier_settings (
    id SERIAL PRIMARY KEY,
    supplier_id INTEGER REFERENCES suppliers(id),
    supplier_name VARCHAR(100) NOT NULL UNIQUE,
    enabled BOOLEAN DEFAULT true,
    priority INTEGER DEFAULT 5,
    timeout_ms INTEGER DEFAULT 30000,
    retry_count INTEGER DEFAULT 3,
    rate_limit_per_minute INTEGER DEFAULT 60,
    custom_settings JSONB DEFAULT '{}',
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Service Connectivity Matrix
CREATE TABLE IF NOT EXISTS service_connectivity_matrix (
    id SERIAL PRIMARY KEY,
    source_service VARCHAR(100) NOT NULL,
    target_service VARCHAR(100) NOT NULL,
    connection_type VARCHAR(50) DEFAULT 'http',
    is_required BOOLEAN DEFAULT true,
    health_endpoint VARCHAR(255),
    last_check_status VARCHAR(50),
    last_check_time TIMESTAMPTZ,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    UNIQUE(source_service, target_service)
);

-- ==========================================
-- ADDITIONAL TABLES FROM REFERENCE SCHEMA
-- Added: 2026-01-10
-- ==========================================

-- Catalog Categories (UUID-based, separate from legacy categories)
CREATE TABLE IF NOT EXISTS catalog_categories (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    name VARCHAR(255) NOT NULL,
    slug VARCHAR(255),
    description TEXT,
    parent_id UUID REFERENCES catalog_categories(id),
    created_at TIMESTAMPTZ DEFAULT NOW(),
    updated_at TIMESTAMPTZ DEFAULT NOW()
);

CREATE INDEX IF NOT EXISTS idx_catalog_categories_name ON catalog_categories(name);
CREATE INDEX IF NOT EXISTS idx_catalog_categories_parent ON catalog_categories(parent_id);

-- Catalog Category Mappings
CREATE TABLE IF NOT EXISTS catalog_category_mappings (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    vendor_name VARCHAR(100) NOT NULL,
    vendor_category VARCHAR(255) NOT NULL,
    normalized_category_id UUID REFERENCES catalog_categories(id),
    created_at TIMESTAMPTZ DEFAULT NOW(),
    updated_at TIMESTAMPTZ DEFAULT NOW()
);

CREATE INDEX IF NOT EXISTS idx_catalog_category_mappings_vendor ON catalog_category_mappings(vendor_name, vendor_category);

-- Catalog Manufacturers (UUID-based)
CREATE TABLE IF NOT EXISTS catalog_manufacturers (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    name VARCHAR(255) NOT NULL,
    normalized_name VARCHAR(255),
    website TEXT,
    supplier_codes JSONB DEFAULT '{}'::jsonb,
    created_at TIMESTAMPTZ DEFAULT NOW(),
    updated_at TIMESTAMPTZ DEFAULT NOW()
);

CREATE UNIQUE INDEX IF NOT EXISTS idx_catalog_manufacturers_name ON catalog_manufacturers(name);

-- Catalog Components (UUID-based, compatible with component_catalog)
CREATE TABLE IF NOT EXISTS catalog_components_table (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    mpn VARCHAR(255) NOT NULL,
    manufacturer VARCHAR(255),
    normalized_mpn VARCHAR(255),
    normalized_manufacturer VARCHAR(255),
    description TEXT,
    category VARCHAR(255),
    subcategory VARCHAR(255),
    category_path TEXT,
    product_family VARCHAR(255),
    product_series VARCHAR(255),
    datasheet_url TEXT,
    image_url TEXT,
    lifecycle_status VARCHAR(50),
    package VARCHAR(100),
    unit_price NUMERIC,
    currency VARCHAR(10),
    price_breaks JSONB,
    moq INTEGER,
    lead_time_days INTEGER,
    stock_status VARCHAR(50),
    supplier_data JSONB,
    specifications JSONB,
    extracted_specs JSONB,
    quality_score NUMERIC,
    quality_metadata JSONB,
    ai_metadata JSONB,
    rohs_compliant BOOLEAN,
    reach_compliant BOOLEAN,
    halogen_free BOOLEAN,
    aec_qualified BOOLEAN,
    eccn_code VARCHAR(50),
    enrichment_source VARCHAR(50),
    api_source VARCHAR(50),
    created_at TIMESTAMPTZ DEFAULT NOW(),
    updated_at TIMESTAMPTZ DEFAULT NOW()
);

CREATE INDEX IF NOT EXISTS idx_catalog_components_table_mpn ON catalog_components_table(mpn);
CREATE INDEX IF NOT EXISTS idx_catalog_components_table_manufacturer ON catalog_components_table(manufacturer);

-- Catalog Component-Manufacturer Junction
CREATE TABLE IF NOT EXISTS catalog_component_manufacturers (
    component_id UUID NOT NULL,
    manufacturer_id UUID NOT NULL,
    PRIMARY KEY (component_id, manufacturer_id)
);

-- CNS Cache Config
CREATE TABLE IF NOT EXISTS cns_cache_config (
    id SERIAL PRIMARY KEY,
    cache_type VARCHAR(100) NOT NULL,
    redis_key_pattern VARCHAR(255),
    ttl_seconds INTEGER DEFAULT 3600,
    max_size_mb INTEGER DEFAULT 100,
    enabled BOOLEAN DEFAULT true,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- CNS Enrichment Config History (Audit)
CREATE TABLE IF NOT EXISTS cns_enrichment_config_history (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    config_key TEXT NOT NULL,
    old_value TEXT,
    new_value TEXT NOT NULL,
    changed_by TEXT NOT NULL,
    changed_at TIMESTAMPTZ DEFAULT NOW(),
    change_reason TEXT,
    ip_address TEXT
);

COMMENT ON TABLE cns_enrichment_config_history IS 'Audit trail of enrichment configuration changes';

-- CNS Supplier Settings (managed via Directus UI)
CREATE TABLE IF NOT EXISTS cns_supplier_settings (
    id SERIAL PRIMARY KEY,
    supplier_name VARCHAR(50) NOT NULL UNIQUE,
    display_name VARCHAR(100),
    supplier_url VARCHAR(255),
    enabled BOOLEAN DEFAULT true,
    priority INTEGER DEFAULT 0,
    api_key TEXT,
    api_secret TEXT,
    base_url TEXT NOT NULL,
    rate_limit_per_minute INTEGER,
    timeout_seconds INTEGER DEFAULT 10,
    supports_parametric_search BOOLEAN DEFAULT false,
    supports_lifecycle_data BOOLEAN DEFAULT true,
    supports_compliance_data BOOLEAN DEFAULT true,
    supports_pricing_breaks BOOLEAN DEFAULT true,
    avg_response_time_ms INTEGER,
    avg_match_confidence NUMERIC(5,2),
    avg_quality_score NUMERIC(5,2),
    success_rate NUMERIC(5,2),
    total_requests_30d INTEGER DEFAULT 0,
    created_at TIMESTAMPTZ DEFAULT NOW(),
    updated_at TIMESTAMPTZ DEFAULT NOW(),
    created_by TEXT,
    notes TEXT
);

COMMENT ON TABLE cns_supplier_settings IS 'Supplier API configuration managed via Directus UI (replaces hardcoded config.py settings)';
COMMENT ON COLUMN cns_supplier_settings.priority IS 'Try suppliers in order: 1=first, 2=second, etc. (0=disabled)';
COMMENT ON COLUMN cns_supplier_settings.api_key IS 'Encrypted API key - handled by application';

-- Component Pricing History
CREATE TABLE IF NOT EXISTS component_pricing_history (
    id SERIAL PRIMARY KEY,
    component_id INTEGER REFERENCES components(id),
    supplier_id VARCHAR(50) NOT NULL,
    quantity_tier INTEGER NOT NULL,
    unit_price NUMERIC(12,4) NOT NULL,
    currency VARCHAR(3) DEFAULT 'USD',
    effective_date TIMESTAMPTZ NOT NULL,
    expiration_date TIMESTAMPTZ,
    is_current BOOLEAN DEFAULT false,
    price_change_percentage NUMERIC(5,2),
    previous_price NUMERIC(12,4),
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

CREATE INDEX IF NOT EXISTS idx_component_pricing_history_component ON component_pricing_history(component_id);
CREATE INDEX IF NOT EXISTS idx_component_pricing_history_supplier ON component_pricing_history(supplier_id);

-- Component Stock Levels
CREATE TABLE IF NOT EXISTS component_stock_levels (
    id SERIAL PRIMARY KEY,
    component_id INTEGER REFERENCES components(id),
    supplier_id VARCHAR(50) NOT NULL,
    stock_quantity INTEGER DEFAULT 0,
    warehouse_location VARCHAR(100),
    min_stock_level INTEGER DEFAULT 10,
    max_stock_level INTEGER DEFAULT 1000,
    reorder_point INTEGER DEFAULT 100,
    lead_time_days INTEGER DEFAULT 0,
    last_updated TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    next_update_scheduled TIMESTAMPTZ
);

CREATE INDEX IF NOT EXISTS idx_component_stock_levels_component ON component_stock_levels(component_id);

-- Component Storage Tracking (Redis vs Database routing)
CREATE TABLE IF NOT EXISTS component_storage_tracking (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    manufacturer_part_number TEXT NOT NULL,
    manufacturer TEXT NOT NULL,
    line_id TEXT NOT NULL,
    storage_location TEXT NOT NULL CHECK (storage_location IN ('database', 'redis')),
    component_catalog_id UUID,
    redis_key TEXT,
    quality_score NUMERIC(5,2),
    metadata JSONB DEFAULT '{}'::jsonb,
    created_at TIMESTAMPTZ DEFAULT NOW(),
    updated_at TIMESTAMPTZ DEFAULT NOW(),
    redis_ttl_seconds INTEGER DEFAULT 259200,
    expires_at TIMESTAMPTZ
);

CREATE INDEX IF NOT EXISTS idx_component_storage_tracking_mpn ON component_storage_tracking(manufacturer_part_number);
CREATE INDEX IF NOT EXISTS idx_component_storage_tracking_location ON component_storage_tracking(storage_location);

COMMENT ON TABLE component_storage_tracking IS 'Tracks storage location (database vs Redis) for each enriched component. Used to route lookups and manage Redis TTL for low-quality components.';
COMMENT ON COLUMN component_storage_tracking.line_id IS 'Unique identifier for the BOM line item (not the BOM line item UUID)';
COMMENT ON COLUMN component_storage_tracking.quality_score IS 'Quality score (0-100) that determined storage routing: >= 80: database (permanent), < 80: redis (temporary, 72hr TTL)';

-- Component Substitution Rules
CREATE TABLE IF NOT EXISTS component_substitution_rules (
    id SERIAL PRIMARY KEY,
    primary_component_id INTEGER REFERENCES components(id),
    substitute_component_id INTEGER REFERENCES components(id),
    compatibility_score NUMERIC(3,2) DEFAULT 1.0,
    notes TEXT,
    is_approved BOOLEAN DEFAULT false,
    approved_by VARCHAR(100),
    approved_date TIMESTAMPTZ,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

CREATE INDEX IF NOT EXISTS idx_component_substitution_primary ON component_substitution_rules(primary_component_id);
CREATE INDEX IF NOT EXISTS idx_component_substitution_substitute ON component_substitution_rules(substitute_component_id);

-- Category Snapshot Audit
CREATE TABLE IF NOT EXISTS category_snapshot_audit (
    id BIGSERIAL PRIMARY KEY,
    run_started TIMESTAMPTZ NOT NULL,
    run_completed TIMESTAMPTZ NOT NULL,
    source_path TEXT NOT NULL,
    truncated BOOLEAN DEFAULT false NOT NULL,
    rows_before BIGINT NOT NULL,
    rows_after BIGINT NOT NULL,
    rows_delta BIGINT NOT NULL,
    rows_flattened BIGINT NOT NULL,
    note TEXT
);

COMMENT ON TABLE category_snapshot_audit IS 'Audit trail for category snapshot loader runs';
COMMENT ON COLUMN category_snapshot_audit.run_started IS 'When the snapshot load started';
COMMENT ON COLUMN category_snapshot_audit.run_completed IS 'When the snapshot load completed';
COMMENT ON COLUMN category_snapshot_audit.source_path IS 'Path to source JSON file';
COMMENT ON COLUMN category_snapshot_audit.truncated IS 'Whether the table was truncated before load';
COMMENT ON COLUMN category_snapshot_audit.rows_flattened IS 'Number of categories flattened from source';

-- Category Source Snapshot
CREATE TABLE IF NOT EXISTS category_source_snapshot (
    source_id BIGINT NOT NULL,
    parent_id BIGINT,
    name TEXT NOT NULL,
    depth INTEGER NOT NULL,
    path TEXT NOT NULL,
    product_count INTEGER,
    raw_payload JSONB NOT NULL,
    ingested_at TIMESTAMPTZ DEFAULT NOW() NOT NULL,
    PRIMARY KEY (source_id)
);

-- Redis Component Snapshot (for Directus visibility)
CREATE TABLE IF NOT EXISTS redis_component_snapshot (
    id UUID PRIMARY KEY DEFAULT gen_random_uuid(),
    redis_key TEXT NOT NULL,
    line_id TEXT NOT NULL,
    mpn TEXT,
    manufacturer TEXT,
    quality_score NUMERIC(5,2),
    component_data JSONB NOT NULL,
    storage_ttl_seconds INTEGER,
    expires_at TIMESTAMPTZ,
    reason_for_redis TEXT,
    can_promote BOOLEAN DEFAULT true,
    promotion_notes TEXT,
    last_synced_at TIMESTAMPTZ DEFAULT NOW(),
    sync_status TEXT DEFAULT 'active',
    created_at TIMESTAMPTZ DEFAULT NOW()
);

CREATE INDEX IF NOT EXISTS idx_redis_component_snapshot_mpn ON redis_component_snapshot(mpn);
CREATE INDEX IF NOT EXISTS idx_redis_component_snapshot_status ON redis_component_snapshot(sync_status);

COMMENT ON TABLE redis_component_snapshot IS 'Snapshot of components stored in Redis (temporary, low-quality) for Directus visibility';
COMMENT ON COLUMN redis_component_snapshot.can_promote IS 'Whether admin can manually promote this component to permanent storage';

-- BOM Queue Config
CREATE TABLE IF NOT EXISTS bom_queue_config (
    id SERIAL PRIMARY KEY,
    queue_name VARCHAR(100) NOT NULL UNIQUE,
    max_concurrent_jobs INTEGER DEFAULT 5,
    retry_attempts INTEGER DEFAULT 3,
    retry_delay_seconds INTEGER DEFAULT 60,
    priority_boost_threshold INTEGER DEFAULT 80,
    enabled BOOLEAN DEFAULT true,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Supplier Rate Limits (if not already created by redis tables)
CREATE TABLE IF NOT EXISTS supplier_rate_limits (
    id SERIAL PRIMARY KEY,
    supplier_name VARCHAR(50) NOT NULL UNIQUE,
    requests_per_second INTEGER DEFAULT 10,
    requests_per_minute INTEGER DEFAULT 60,
    requests_per_hour INTEGER DEFAULT 1000,
    concurrent_requests INTEGER DEFAULT 5,
    burst_limit INTEGER DEFAULT 20,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- Rate Limit Config
CREATE TABLE IF NOT EXISTS rate_limit_config (
    id SERIAL PRIMARY KEY,
    service_name VARCHAR(100) NOT NULL UNIQUE,
    endpoint_pattern VARCHAR(255),
    max_requests_per_minute INTEGER DEFAULT 60,
    max_requests_per_hour INTEGER DEFAULT 1000,
    burst_size INTEGER DEFAULT 10,
    enabled BOOLEAN DEFAULT true,
    created_at TIMESTAMPTZ DEFAULT CURRENT_TIMESTAMP
);

-- ==========================================
-- VIEWS FOR DIRECTUS COMPATIBILITY
-- ==========================================

-- Directus Redis Components View
CREATE OR REPLACE VIEW directus_redis_components AS
SELECT
    id,
    mpn,
    manufacturer,
    quality_score,
    'redis'::TEXT AS storage_location,
    CASE
        WHEN sync_status = 'expired' THEN 'Expired'
        WHEN expires_at < NOW() THEN 'Expired'
        WHEN expires_at < (NOW() + INTERVAL '24 hours') THEN 'Expiring Soon'
        ELSE 'Temporary'
    END AS storage_status,
    expires_at,
    EXTRACT(epoch FROM (expires_at - NOW())) / 3600 AS ttl_hours,
    false AS is_permanent,
    reason_for_redis,
    can_promote,
    promotion_notes,
    sync_status,
    created_at,
    last_synced_at AS updated_at,
    component_data
FROM redis_component_snapshot
WHERE sync_status IN ('active', 'expired');

COMMENT ON VIEW directus_redis_components IS 'Redis components view for Directus dashboard (Directus-only)';

-- Unified Component View (Database + Redis)
CREATE OR REPLACE VIEW directus_component_unified AS
SELECT
    id,
    manufacturer_part_number AS mpn,
    manufacturer,
    quality_score,
    'database'::TEXT AS storage_location,
    'Permanent'::TEXT AS storage_status,
    NULL::TIMESTAMPTZ AS expires_at,
    NULL::NUMERIC AS ttl_hours,
    true AS is_permanent,
    NULL::TEXT AS reason_for_redis,
    true AS can_promote,
    NULL::TEXT AS promotion_notes,
    'stored'::TEXT AS sync_status,
    created_at,
    updated_at,
    jsonb_build_object(
        'category', category,
        'subcategory', subcategory,
        'description', description,
        'datasheet_url', datasheet_url,
        'image_url', image_url,
        'lifecycle_status', lifecycle_status,
        'rohs_compliant', rohs_compliant,
        'reach_compliant', reach_compliant,
        'aec_qualified', aec_qualified,
        'halogen_free', halogen_free,
        'unit_price', unit_price,
        'currency', currency,
        'stock_status', stock_status,
        'stock_quantity', stock_quantity,
        'packaging', packaging,
        'minimum_order_quantity', minimum_order_quantity,
        'eccn_code', eccn_code,
        'hts_code', hts_code,
        'supplier_part_number', supplier_part_number,
        'supplier_name', supplier_name,
        'enrichment_source', enrichment_source,
        'enrichment_version', enrichment_version,
        'last_enriched_at', last_enriched_at,
        'quality_metadata', quality_metadata,
        'supplier_data', supplier_data,
        'parameters', parameters
    ) AS component_data
FROM component_catalog
UNION ALL
SELECT * FROM directus_redis_components;

COMMENT ON VIEW directus_component_unified IS 'Unified Directus view showing permanent (database) and temporary (Redis) components.';

-- Seed default CNS supplier settings
INSERT INTO cns_supplier_settings (supplier_name, display_name, base_url, enabled, priority, timeout_seconds, supports_lifecycle_data)
VALUES
    ('digikey', 'DigiKey', 'https://api.digikey.com', true, 1, 30, true),
    ('mouser', 'Mouser Electronics', 'https://api.mouser.com', true, 2, 30, true),
    ('element14', 'element14 / Newark', 'https://api.element14.com', true, 3, 30, true),
    ('arrow', 'Arrow Electronics', 'https://api.arrow.com', true, 4, 30, true),
    ('avnet', 'Avnet', 'https://api.avnet.com', false, 5, 30, true)
ON CONFLICT (supplier_name) DO NOTHING;

-- Seed default Redis configs
INSERT INTO redis_cns_config (service_name, cache_ttl_seconds, lookup_cache_ttl_seconds, batch_cache_ttl_seconds)
VALUES ('cns-service', 3600, 86400, 1800)
ON CONFLICT DO NOTHING;

INSERT INTO redis_rate_limits (supplier_name, requests_per_hour, requests_per_minute)
VALUES
    ('digikey', 10000, 200),
    ('mouser', 8000, 150),
    ('element14', 5000, 100),
    ('arrow', 3000, 60)
ON CONFLICT DO NOTHING;

INSERT INTO redis_key_expiration_policy (key_pattern, ttl_seconds, eviction_policy, description)
VALUES
    ('component:*', 86400, 'allkeys-lru', 'Component cache - 24 hour TTL'),
    ('enrichment:*', 3600, 'volatile-lru', 'Enrichment results - 1 hour TTL'),
    ('supplier:*', 43200, 'volatile-lru', 'Supplier responses - 12 hour TTL'),
    ('session:*', 3600, 'volatile-ttl', 'User sessions - 1 hour TTL'),
    ('rate:*', 60, 'volatile-ttl', 'Rate limit counters - 1 minute TTL')
ON CONFLICT DO NOTHING;

INSERT INTO supplier_settings (supplier_name, enabled, priority, timeout_ms, rate_limit_per_minute)
VALUES
    ('digikey', true, 1, 30000, 60),
    ('mouser', true, 2, 30000, 60),
    ('element14', true, 3, 30000, 30),
    ('arrow', true, 4, 30000, 30),
    ('avnet', false, 5, 30000, 30)
ON CONFLICT (supplier_name) DO NOTHING;

-- ==========================================
-- VERIFICATION
-- ==========================================

DO $$
DECLARE
    table_count INTEGER;
    view_count INTEGER;
    function_count INTEGER;
    manufacturer_count INTEGER;
    category_count INTEGER;
    supplier_count INTEGER;
    config_count INTEGER;
BEGIN
    -- Count tables
    SELECT COUNT(*) INTO table_count
    FROM information_schema.tables
    WHERE table_schema = 'public'
      AND table_type = 'BASE TABLE';

    -- Count views
    SELECT COUNT(*) INTO view_count
    FROM information_schema.views
    WHERE table_schema = 'public';

    -- Count functions
    SELECT COUNT(DISTINCT proname) INTO function_count
    FROM pg_proc
    WHERE pronamespace = 'public'::regnamespace
      AND prokind = 'f';

    -- Count seed data
    SELECT COUNT(*) INTO manufacturer_count FROM manufacturers;
    SELECT COUNT(*) INTO category_count FROM categories;
    SELECT COUNT(*) INTO supplier_count FROM suppliers;
    SELECT COUNT(*) INTO config_count FROM cns_enrichment_config;

    RAISE NOTICE '';
    RAISE NOTICE '========================================';
    RAISE NOTICE 'Components-V2 MASTER MIGRATION COMPLETE';
    RAISE NOTICE '========================================';
    RAISE NOTICE '';
    RAISE NOTICE 'DATABASE OBJECTS:';
    RAISE NOTICE '  - Tables: %', table_count;
    RAISE NOTICE '  - Views: %', view_count;
    RAISE NOTICE '  - Functions: %', function_count;
    RAISE NOTICE '';
    RAISE NOTICE 'TABLES CREATED:';
    RAISE NOTICE '  1. manufacturers (master list)';
    RAISE NOTICE '  2. categories (hierarchical taxonomy)';
    RAISE NOTICE '  3. suppliers (API-enabled distributors)';
    RAISE NOTICE '  4. supplier_tokens (OAuth tokens)';
    RAISE NOTICE '  5. components (original catalog with manufacturer_id FK)';
    RAISE NOTICE '  6. component_catalog (CNS enrichment catalog - PRIMARY)';
    RAISE NOTICE '  7. component_pricing (supplier pricing data)';
    RAISE NOTICE '  8. vendor_category_mappings (category normalization)';
    RAISE NOTICE '  9. enrichment_config (per-org enrichment settings)';
    RAISE NOTICE ' 10. cns_enrichment_config (runtime config)';
    RAISE NOTICE ' 11. supplier_enrichment_responses (raw API responses)';
    RAISE NOTICE ' 12. column_mapping_templates (BOM upload templates)';
    RAISE NOTICE ' 13. audit_enrichment_runs (enrichment audit log)';
    RAISE NOTICE ' 14. audit_field_comparisons (field-level changes)';
    RAISE NOTICE ' 15. audit_supplier_quality (daily supplier metrics)';
    RAISE NOTICE '';
    RAISE NOTICE 'VIEWS CREATED:';
    RAISE NOTICE '  1. catalog_components (ORM compatibility for component_catalog)';
    RAISE NOTICE '';
    RAISE NOTICE 'FUNCTIONS CREATED:';
    RAISE NOTICE '  1. update_updated_at_column()';
    RAISE NOTICE '  2. update_component_catalog_updated_at()';
    RAISE NOTICE '  3. update_column_mapping_templates_updated_at()';
    RAISE NOTICE '  4. update_modified_timestamp()';
    RAISE NOTICE '  5. update_search_vector()';
    RAISE NOTICE '  6. ensure_single_default_template()';
    RAISE NOTICE '  7. update_supplier_quality_stats(target_date)';
    RAISE NOTICE '';
    RAISE NOTICE 'SEED DATA INSERTED:';
    RAISE NOTICE '  - Manufacturers: % (STMicroelectronics, TI, Murata, Samsung, Yageo, TDK, Vishay, ON Semi, Microchip, NXP)', manufacturer_count;
    RAISE NOTICE '  - Categories: % (Resistors, Capacitors, Inductors, ICs, Connectors, Diodes, Transistors, MCUs, Sensors, Power)', category_count;
    RAISE NOTICE '  - Suppliers: % (DigiKey, Mouser, Arrow, Avnet, Newark)', supplier_count;
    RAISE NOTICE '  - Config entries: % (enrichment thresholds, circuit breaker, retry)', config_count;
    RAISE NOTICE '';
    RAISE NOTICE 'KEY FEATURES:';
    RAISE NOTICE '  - Dual catalog tables: components (FK-based) + component_catalog (string-based)';
    RAISE NOTICE '  - CNS enrichment uses component_catalog as primary source';
    RAISE NOTICE '  - catalog_components VIEW maps to component_catalog for ORM';
    RAISE NOTICE '  - Complete audit trail with field-level change tracking';
    RAISE NOTICE '  - Organization-scoped column mapping templates';
    RAISE NOTICE '  - Daily supplier quality aggregation';
    RAISE NOTICE '  - OAuth token management for supplier APIs';
    RAISE NOTICE '  - Automated triggers for updated_at timestamps';
    RAISE NOTICE '';
    RAISE NOTICE 'READY FOR CNS SERVICE';
    RAISE NOTICE '========================================';
    RAISE NOTICE '';
END$$;
