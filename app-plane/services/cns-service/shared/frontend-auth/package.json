{
  "name": "@components-platform/frontend-auth",
  "version": "1.0.0",
  "description": "Shared Auth0 + Supabase authentication for Components Platform frontends",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./auth0": {
      "import": "./dist/auth0/index.js",
      "types": "./dist/auth0/index.d.ts"
    },
    "./supabase": {
      "import": "./dist/supabase/index.js",
      "types": "./dist/supabase/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "peerDependencies": {
    "@auth0/auth0-react": "^2.9.0",
    "@mui/material": "^5.15.6",
    "@mui/icons-material": "^5.15.6",
    "@supabase/supabase-js": "^2.78.0",
    "react": "^18.2.0",
    "react-admin": "^4.16.9",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "typescript": "^5.2.2"
  }
}
